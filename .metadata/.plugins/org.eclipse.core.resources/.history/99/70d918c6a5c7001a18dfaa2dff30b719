import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

//Exercicio1:	Leia um valor inteiro X (1 <= X <= 1000). Em seguida mostre os ímpares de 1 até X, um valor por linha, inclusive o
//		X, se for o caso.
		Scanner sc = new Scanner(System.in);
		System.out.println("Digite um numero:");
		int x = sc.nextInt();
		int result;

		for (int j = x; j <= x; j--) {
			result = x % 3;
			if(result==2) {
//				System.out.println("O numero é ímpar:" + x);
				x--;
			}
			System.out.println("O numero é ímpar:" + x);

		}
		x = sc.nextInt();

//		Final da classe
		sc.close();

	}

}
